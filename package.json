{
  "name": "valorant-props-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/web",
    "packages/api"
  ],
  "scripts": {
    "dev:web": "npm --workspace @app/web run dev",
    "dev:api": "npm --workspace @app/api run dev",
    "build:api": "npm --workspace @app/api run build",
    "prisma:generate": "npm --workspace @app/api run prisma:generate",
    "prisma:push": "npm --workspace @app/api run prisma:push",
    "build:verify": "pnpm --filter ./packages/web build && pnpm --filter ./packages/api build"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706",
  "dependencies": {
    "@supabase/auth-ui-shared": "^0.1.8"
  }
}
